package SelfProgramming;

public class FlagControlledLoop {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		boolean found = false;
		int x=0;
		while (!found){
			x++;
			System.out.println("X is "+x);
		if(x==10)
			found = true;
		}
		System.out.println("GOOD JOB!");
	}

}
